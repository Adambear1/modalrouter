  "dependencies": {
    "bootstrap": "^4.6.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "if-env": "^1.0.4",
    "react-bootstrap": "^1.5.0"
  },
